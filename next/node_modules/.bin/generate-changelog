#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/generate-changelog@1.8.0/node_modules/generate-changelog/bin/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/generate-changelog@1.8.0/node_modules/generate-changelog/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/generate-changelog@1.8.0/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/generate-changelog@1.8.0/node_modules/generate-changelog/bin/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/generate-changelog@1.8.0/node_modules/generate-changelog/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/generate-changelog@1.8.0/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../generate-changelog/bin/generate" "$@"
else
  exec node  "$basedir/../generate-changelog/bin/generate" "$@"
fi
