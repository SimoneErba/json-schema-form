#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/@antfu+eslint-config@3.14.0_@typescript-eslint+utils@8.20.0_eslint@9.18.0_typescript@5.7.3__@_seihwdgjrh4drfht6dai3uw26q/node_modules/@antfu/eslint-config/bin/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/@antfu+eslint-config@3.14.0_@typescript-eslint+utils@8.20.0_eslint@9.18.0_typescript@5.7.3__@_seihwdgjrh4drfht6dai3uw26q/node_modules/@antfu/eslint-config/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/@antfu+eslint-config@3.14.0_@typescript-eslint+utils@8.20.0_eslint@9.18.0_typescript@5.7.3__@_seihwdgjrh4drfht6dai3uw26q/node_modules/@antfu/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/@antfu+eslint-config@3.14.0_@typescript-eslint+utils@8.20.0_eslint@9.18.0_typescript@5.7.3__@_seihwdgjrh4drfht6dai3uw26q/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/@antfu+eslint-config@3.14.0_@typescript-eslint+utils@8.20.0_eslint@9.18.0_typescript@5.7.3__@_seihwdgjrh4drfht6dai3uw26q/node_modules/@antfu/eslint-config/bin/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/@antfu+eslint-config@3.14.0_@typescript-eslint+utils@8.20.0_eslint@9.18.0_typescript@5.7.3__@_seihwdgjrh4drfht6dai3uw26q/node_modules/@antfu/eslint-config/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/@antfu+eslint-config@3.14.0_@typescript-eslint+utils@8.20.0_eslint@9.18.0_typescript@5.7.3__@_seihwdgjrh4drfht6dai3uw26q/node_modules/@antfu/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/@antfu+eslint-config@3.14.0_@typescript-eslint+utils@8.20.0_eslint@9.18.0_typescript@5.7.3__@_seihwdgjrh4drfht6dai3uw26q/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
else
  exec node  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
fi
